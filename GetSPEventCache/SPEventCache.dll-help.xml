    <command:examples>
      <command:example>
        <maml:title>Example 1: Retrieves all events between two specified time frames.</maml:title>
        <maml:introduction>
          <maml:para />
        </maml:introduction>
        <dev:code>
          PS C:\&gt;Get-SPEventCache -DatabaseName SharePointCDB1 -DatabaseServer SPSQL -EventTime '06/25/2015 14:00' -EndTime '06/27/2015 8:00'
        </dev:code>
        <dev:remarks></dev:remarks>
        <command:commandLines>
          <command:commandLine>
            <command:commandText />
          </command:commandLine>
        </command:commandLines>
      </command:example>
      <command:example>
        <maml:title>Example 2: Retrieves all events with a specific Correlation Id and Modified By.</maml:title>
        <maml:introduction>
          <maml:para />
        </maml:introduction>
        <dev:code>
          PS C:\&gt;Get-SPEventCache -DatabaseName SharePointCDB1 -DatabaseServer SPSQL -CorrelationId '3c4c149d-1f5a-200b-f1ff-af2bc00eded1' -ModifiedBy 'John'
        </dev:code>
      </command:example>
      <command:example>
        <maml:title>Example 3: Retrieves List Items that have been deleted from a specific URL.</maml:title>
        <maml:introduction>
          <maml:para />
        </maml:introduction>
        <dev:code>
          PS C:\&gt;Get-SPEventCache -DatabaseName SharePointCDB1 -DatabaseServer SPSQL -ItemFullUrl 'sites/teams/' | where {$_.EventType -contain 'ListItem' -and $_.ObjectType -match 'Delete'}
        </dev:code>
      </command:example>
    </command:examples>
