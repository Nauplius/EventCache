    <command:details>
      <command:name>Get-SPEventCache</command:name>
      <maml:description>
        <maml:para>Get-SPEventCache retreives the EventCache from the specified database.</maml:para>
      </maml:description>
      <maml:copyright>
        <maml:para>2015, Nauplius</maml:para>
      </maml:copyright>
      <command:verb>Get</command:verb>
      <command:noun>SPEventCache</command:noun>
    </command:details>
    <maml:description>
      <maml:para>Get-SPEventCache retreives the EventCache from the specified database.</maml:para>
    </maml:description>
    <command:syntax>
      <command:syntaxItem>
        <maml:name>Get-SPEventCache</maml:name>
        
        <command:parameter required="true" position="named" aliases="">
          <maml:name>DatabaseName</maml:name>
