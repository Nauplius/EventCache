            <maml:para>Start time to look at events.</maml:para>
          </maml:description>
          <command:parameterValue required="true">DateTime</command:parameterValue>
        </command:parameter>

        <command:parameter required="false" position="named" aliases="">
          <maml:name>EventEnd</maml:name>
          <maml:description>
            <maml:para>End time to look at events.</maml:para>
          </maml:description>
          <command:parameterValue required="true">DateTime</command:parameterValue>
        </command:parameter>

        <command:parameter required="false" position="named" aliases="">
          <maml:name>OrderBy</maml:name>
          <maml:description>
