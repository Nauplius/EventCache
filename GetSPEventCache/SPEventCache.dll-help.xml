          <command:parameterValue required="true">String</command:parameterValue>
        </command:parameter>
        
        <command:parameter required="true" position="named" aliases="">
          <maml:name>DatabaseServer</maml:name>
          <maml:description>
            <maml:para>Database server containing SharePoint database.</maml:para>
          </maml:description>
          <command:parameterValue required="true">String</command:parameterValue>
        </command:parameter>
      </command:syntaxItem>
    </command:syntax>

    <command:parameter required="true" position="named" aliases="">
