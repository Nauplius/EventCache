        <command:parameter required="false" position="named" aliases="">
          <maml:name>SortOrder</maml:name>
          <maml:description>
            <maml:para>Sort the results.</maml:para>
          </maml:description>
          <command:parameterValue required="true">Sort</command:parameterValue>
        </command:parameter>

        <command:parameter required="false" position="named" aliases="">
          <maml:name>TopRecords</maml:name>
          <maml:description>
            <maml:para>Return only the specified top records.</maml:para>
          </maml:description>
          <command:parameterValue required="true">Integer</command:parameterValue>
        </command:parameter>

        <command:parameter required="false" position="named" aliases="">
          <maml:name>CorrelationId</maml:name>
          <maml:description>
            <maml:para>Return records matching the specified Correlation Id.</maml:para>
          </maml:description>
          <command:parameterValue required="true">String</command:parameterValue>
        </command:parameter>

        <command:parameter required="false" position="named" aliases="">
          <maml:name>WebId</maml:name>
          <maml:description>
            <maml:para>Return records matching the specified SPWeb Id.</maml:para>
          </maml:description>
          <command:parameterValue required="true">String</command:parameterValue>
        </command:parameter>

        <command:parameter required="false" position="named" aliases="">
          <maml:name>SiteId</maml:name>
          <maml:description>
            <maml:para>Return records matching the specified SPSite Id.</maml:para>
          </maml:description>
          <command:parameterValue required="true">String</command:parameterValue>
        </command:parameter>

        <command:parameter required="false" position="named" aliases="">
          <maml:name>ListId</maml:name>
          <maml:description>
            <maml:para>Return records matching the specified SPList Id.</maml:para>
          </maml:description>
          <command:parameterValue required="true">String</command:parameterValue>
        </command:parameter>

        <command:parameter required="false" position="named" aliases="">
          <maml:name>DocId</maml:name>
          <maml:description>
            <maml:para>Return records matching the specified Document Id.</maml:para>
          </maml:description>
          <command:parameterValue required="true">String</command:parameterValue>
        </command:parameter>

        <command:parameter required="false" position="named" aliases="">
          <maml:name>ItemName</maml:name>
          <maml:description>
            <maml:para>Return records matching the specified item name. Parameter uses Logical Operator LIKE in T-SQL query.</maml:para>
          </maml:description>
          <command:parameterValue required="true">String</command:parameterValue>
        </command:parameter>

        <command:parameter required="false" position="named" aliases="">
          <maml:name>ItemFullUrl</maml:name>
          <maml:description>
            <maml:para>Return records matching the specified item URL. Parameter uses Logical Operator LIKE in T-SQL query.</maml:para>
          </maml:description>
          <command:parameterValue required="true">String</command:parameterValue>
        </command:parameter>

        <command:parameter required="false" position="named" aliases="">
          <maml:name>ModifiedBy</maml:name>
          <maml:description>
            <maml:para>Return records matching the name provided for the Modified By field. Parameter uses Logical Operator LIKE in T-SQL query.</maml:para>
          </maml:description>
          <command:parameterValue required="true">String</command:parameterValue>
        </command:parameter>

        <command:parameter required="false" position="named" aliases="">
          <maml:name>Username</maml:name>
          <maml:description>
            <maml:para>Connects to the database using the specified SQL Server user.</maml:para>
          </maml:description>
          <command:parameterValue required="true">String</command:parameterValue>
        </command:parameter>

        <command:parameter required="false" position="named" aliases="">
          <maml:name>Password</maml:name>
          <maml:description>
            <maml:para>Connects to the database using the specified SQL Server user's password.</maml:para>
          </maml:description>
          <command:parameterValue required="true">SecureString</command:parameterValue>
        </command:parameter>
